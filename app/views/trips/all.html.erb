  
  <tbody>
  <% current_user.repositories.each do |repo| %>
    <tr>
      <% repo.trips.each do |trip| %>
        <td><%= trip.title %></p>
        <td><%= trip.content %></p>
        <td><%= trip.repository_id %></td>
        <td><%= link_to 'Show', trip %></td>
        <td><%= link_to 'Edit', edit_trip_path(trip) %></td>
        <td><%= link_to 'Destroy', trip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>