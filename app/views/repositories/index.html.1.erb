<style>

  #left {
      width: 15%;
      height: 100%;
      float: left;
      background: #FFD700;
  }

  #right {
      width: 85%;
      height: 100%;
      float: left;
      background: #FFA500;
  }

</style>

<p id="notice"><%= notice %></p>

<%= current_user.email %> 님 환영합니다. 
<a href="/users/sign_out" data-method="delete">로그아웃</a>


<h1>Listing Repositories</h1>


<body>


  <div style="100%">
    <div id="left">하하</div>
    <div id="right">
        <table>
          <thead>
            <tr>
              <th>Title</th>
              <th>Content</th>
              <!--<th>User</th>-->
              <th colspan="3"></th>
            </tr>
          </thead>
        
          <tbody>
            <% @repositories.each do |repository| %>
              <tr>
                <td><%= repository.title %></td>
                <td><%= repository.content %></td>
                <!--<td><%#= repository.user_id %></td>-->
                <td><%= link_to 'Show', repository %></td>
                <td><%= link_to 'Edit', edit_repository_path(repository) %></td>
                <td><%= link_to 'Destroy', repository, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
        <br>
      </div>
  <div>    

    
<%= link_to 'New Repository', new_repository_path %>

</body>
